class Solution:
    def generateParenthesis(self, n: int) -> List[str]:
        res = []
        stack = []

        def backtrack(open, closed):
            #valid answer
            if open == closed == n:
                res.append("".join(stack))

            #add open
            if open < n:
                stack.append("(")
                backtrack(open + 1, closed)
                stack.pop()

            #add closing
            if closed < open:
                stack.append(")")
                backtrack(open, closed + 1)
                stack.pop()
        
        backtrack(0, 0)
        return res
